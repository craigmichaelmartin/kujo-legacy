{
  "name": "kujo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "css:autoprefixer": "postcss --use autoprefixer public/css/app.css -o public/css/app.css",
    "css:build": "run-s css:transpile css:autoprefixer css:concatenate css:minify",
    "css:concatenate": "cat node_modules/bootstrap/dist/css/bootstrap.min.css node_modules/material-design-lite/dist/material.indigo-pink.min.css  node_modules/slick-carousel/slick/slick.css public/css/app.css > public/css/bundle.css",
    "css:minify": "cssmin public/css/bundle.css > public/css/bundle.min.css",
    "css:transpile": "node-sass public/sass/main.scss public/css/app.css",
    "css:lint": "sass-lint -v -q -c public/sass/.sass-lint.yml public/sass/components/*.scss",
    "css:watch": "watch \"npm run css:build\" public/sass/ node_modules",
    "dirs:create": "mkdirp public/css public/compiledES2015",
    "dirs:clean": "rimraf public/css public/compiledES2015",
    "dirs:fresh": "run-s dirs:clean dirs:create",
    "js:lint": "eslint .",
    "js:test": "echo \"Tests no implemented yet.\"",
    "js:build": "browserify public/javascript/main.js -dv | uglifyjs > public/compiledES2015/bundle.js",
    "js:watch": "watchify public/javascript/main.js -o public/compiledES2015/bundle.js -dv",
    "lint": "run-s css:lint js:lint",
    "postinstall": "run-s build",
    "build": "run-p css:build js:build",
    "prestart": "npm run build",
    "start": "node ./bin/www",
    "test": "run-s lint js:test",
    "watch": "run-p css:watch js:watch"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "backbone": "^1.3.3",
    "body-parser": "~1.13.2",
    "bootstrap": "3.3.6",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "es5-shim": "^4.5.9",
    "express": "~4.13.1",
    "font-awesome": "^4.6.3",
    "jade": "~1.11.0",
    "jquery": "2.1.4",
    "material-design-lite": "^1.1.3",
    "morgan": "~1.6.1",
    "mustache-express": "^1.2.2",
    "sass-lint": "^1.7.0",
    "serve-favicon": "~2.3.0",
    "slick-carousel": "^1.6.0",
    "underscore": "^1.8.3",
    "autoprefixer": "~6.3.3",
    "babel": "~5.8.19",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-preset-es2015": "~6.5.0",
    "babelify": "^7.2.0",
    "brfs": "1.4.0",
    "browserify": "12.0.2",
    "catw": "1.0.1",
    "chai": "^3.5.0",
    "csscomb": "~3.1.8",
    "csslint": "~0.10.0",
    "cssmin": "^0.4.3",
    "eslint": "^2.8.0",
    "jasmine-core": "~2.3.4",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "~6.0.1",
    "karma-browserify": "v5.0.1",
    "karma-chai": "v0.1.0",
    "karma-chrome-launcher": "~0.2.0",
    "karma-coverage": "~0.5.3",
    "karma-firefox-launcher": "~0.1.6",
    "karma-ie-launcher": "~0.2.0",
    "karma-jasmine": "~0.3.6",
    "karma-mocha": "~0.2.0",
    "karma-opera-launcher": "~0.3.0",
    "karma-phantomjs2-launcher": "^0.5.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-sauce-launcher": "^0.2.3",
    "karma-sinon": "v1.0.3",
    "karma-sinon-chai": "~1.0.0",
    "less": "~2.5.1",
    "less-plugin-clean-css": "",
    "mkdirp": "^0.5.1",
    "mocha": "~2.3.0",
    "mockery": "^1.7.0",
    "node-inspector": "^0.12.8",
    "node-sass": "~3.4.2",
    "npm-run-all": "^2.0.0",
    "phantomjs-prebuilt": "^2.1.5",
    "postcss-cli": "~2.5.1",
    "rimraf": "^2.5.3",
    "sinon": "^1.17.3",
    "uglify-js": "v2.4.24",
    "watch": "~0.16.0",
    "watchify": "^3.3.1"
  },
  "browserify": {
    "debug": "true",
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "transform-es3-member-expression-literals",
            "transform-es3-property-literals"
          ]
        }
      ],
      "brfs"
    ]
  }
}
